        function loadLabels() {
            $.get("BindZPLFiles", function (data) {
                $('.ZPLlblCls').append('<option value="">@TnT.LangResource.GlobalRes.JobCreateDrpLabel</option>');
                $.each(data.split(','), function (key, value) {
                    $('.ZPLlblCls').append('<option value="' + value + '">' + value.slice(0, -4) + '</option>');
                });
            });
        }
        @{
            var secs = Convert.ToInt32(@Utilities.getAppSettings("UpdateUserTimeInSec"));
            TimeSpan spn = new TimeSpan(0, 0, secs);
        }
        window.setInterval(function () {
            updateUserData();
        },@spn.TotalMilliseconds);


        function updateUserData() {

            $.ajax({
                url: '@(Url.Action("updateUData", "Account"))',
                type: "POST",
                dataType: 'json'
            });
        }
