    <script>
        idleTimer = null;
        idleState = false;
        idleWait = (@Utilities.getAppSettings("ExpirationTime") *60) * 1000;
        //idleWait = 30000;
        //console.log(idleWait);
        (function ($) {

            $(document).ready(function () {

                $('*').bind('mousemove keydown scroll', function () {

                    clearTimeout(idleTimer);

                    if (idleState == true) {

                        // Reactivated event
                        console.log("Session continued.");
                    }

                    idleState = false;

                    idleTimer = setTimeout(function () {

                        // Idle Event

                        console.log("You've been idle for " + idleWait/1000 + " seconds.");

                        window.location.href = "@Url.Action("Logout","Account",new { returnUrl = HttpContext.Current.Request.Url.AbsolutePath})";
                        idleState = true; }, idleWait);
                });

                $("body").trigger("mousemove");

            });
        }) (jQuery)

        @{


        }


    </script>
