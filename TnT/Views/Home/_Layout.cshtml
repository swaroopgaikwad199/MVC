                    }
                    else {
                        $('#modal-rpt-data').html(data);
                        $("#showRPTBx").trigger("click");
                    }
                },
                error: function (data) {

                    toastr.warning("@TnT.LangResource.GlobalRes.toastrCustomerCreateError");

                }
            });
